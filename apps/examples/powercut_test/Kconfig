config EXAMPLES_POWERCUT_TEST
	bool "\"Powercut Test!\" example"
	default n
	---help---
		Enable the \"Powercut Test!\" example

config USER_ENTRYPOINT
	string
	default "powercut_main" if ENTRY_POWERCUT_TEST
